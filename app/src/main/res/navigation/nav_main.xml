<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_main"
    app:startDestination="@id/mapFragment">

    <fragment
        android:id="@+id/mapFragment"
        android:name="com.example.proyectofinal.fragments.MainActivity.MapFragment"
        android:label="fragment_map"
        tools:layout="@layout/fragment_map" >
        <argument
            android:name="filtros"
            app:argType="string[]" />
        <action
            android:id="@+id/action_mapFragment_to_professionalProfileFragment"
            app:destination="@id/professionalProfileFragment" />
    </fragment>
    <fragment
        android:id="@+id/hiringsFragment"
        android:name="com.example.proyectofinal.fragments.HiringsFragment"
        android:label="fragment_my_hires"
        tools:layout="@layout/fragment_hirings" />
    <fragment
        android:id="@+id/userProfileFragment"
        android:name="com.example.proyectofinal.fragments.UserProfileFragment"
        android:label="fragment_user_profile"
        tools:layout="@layout/fragment_user_profile" />
    <fragment
        android:id="@+id/professionalProfileFragment"
        android:name="com.example.proyectofinal.fragments.ProfessionalProfileFragment"
        android:label="fragment_professional_profile"
        tools:layout="@layout/fragment_professional_profile" >
        <argument
            android:name="profId"
            app:argType="string" />
    </fragment>
</navigation>